{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/auth/(.*)", "dest": "api/index.py" },
    { "src": "/api/(.*)", "dest": "api/index.py" },
    { "src": "/book/(.*)", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "public/$1" }
  ]
}
